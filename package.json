{
  "name": "prexvis",
  "version": "0.1.0",
  "bin": "app.js",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "pack": "pkg package.json -t latest-linux-x64 && tar -czf prexvis-$npm_package_version.tgz --transform s/^/prexvis-$npm_package_version\\\\// bin data prexvis",
    "clean": "rm -f prexvis prexvis-$npm_package_version.tgz"
  },
  "description": "Visualization for P-Rex",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clemy/prexvis.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clemy/prexvis/issues"
  },
  "homepage": "https://github.com/clemy/prexvis#readme",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^2.3.0",
    "tmp-promise": "^2.0.2"
  },
  "devDependencies": {
    "pkg": "^4.4.5"
  },
  "pkg": {
    "assets": [
      "static/**/*"
    ]
  }
}
